
CREATE TABLE participants (
    id UUID NOT NULL,
    group_id UUID NOT NULL,
    name VARCHAR(200) NOT NULL,
    username VARCHAR(200) NOT NULL UNIQUE,
    PRIMARY KEY (id),
    FOREIGN KEY (group_id) REFERENCES groups(id)
);


CREATE TABLE groups (
    id UUID NOT NULL,
    name VARCHAR(200) NOT NULL,

    PRIMARY KEY (id)
);


